{
    "name": "s2/admin-yard",
    "description": "Simple admin panel without heavy dependencies",
    "type": "library",
    "authors": [
        {
            "name": "Roman Parpalak",
            "email": "roman@parpalak.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "S2\\AdminYard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "league/commonmark": "^2.4",
        "codeception/codeception": "^5.1",
        "symfony/error-handler": "^7.0",
        "codeception/module-asserts": "^3.0",
        "codeception/module-phpbrowser": "^3.0"
    },
    "require": {
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "symfony/http-foundation": "^7.0",
        "symfony/translation-contracts": "^3.5",
        "symfony/event-dispatcher": "^7.0"
    },
    "suggest": {
        "ext-pdo": "*"
    }
}
